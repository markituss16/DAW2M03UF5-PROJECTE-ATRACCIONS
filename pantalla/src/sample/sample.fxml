<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.cell.*?>
<?import javafx.collections.*?>
<?import sample.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.Controller">
    <children>
        <Button fx:id="boto" onAction="#carregar" text="carrega dades"/>
        <TableView fx:id="taula" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn editable="false"
                             prefWidth="150.0" sortable="false" text="idEntrada">
                    <cellValueFactory>
                        <PropertyValueFactory property="idEntrada" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="150.0" text="categoriaEntrada">
                    <cellValueFactory>
                        <PropertyValueFactory property="categoriaEntrada" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="150.0" text="preu">
                    <cellValueFactory>
                        <PropertyValueFactory property="preu" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="150.0" text="estatCompra">
                    <cellValueFactory>
                        <PropertyValueFactory property="estatCompra" />
                    </cellValueFactory>
                </TableColumn>
            </columns>

            <items>
                <FXCollections fx:factory="observableArrayList">
                </FXCollections>
            </items>

        </TableView>
    </children>
</VBox>
